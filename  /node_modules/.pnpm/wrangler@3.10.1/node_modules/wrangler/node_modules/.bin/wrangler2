#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nevendrean/code/elixir/up_img/ /node_modules/.pnpm/wrangler@3.10.1/node_modules/wrangler/bin/node_modules:/Users/nevendrean/code/elixir/up_img/ /node_modules/.pnpm/wrangler@3.10.1/node_modules/wrangler/node_modules:/Users/nevendrean/code/elixir/up_img/ /node_modules/.pnpm/wrangler@3.10.1/node_modules:/Users/nevendrean/code/elixir/up_img/ /node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nevendrean/code/elixir/up_img/ /node_modules/.pnpm/wrangler@3.10.1/node_modules/wrangler/bin/node_modules:/Users/nevendrean/code/elixir/up_img/ /node_modules/.pnpm/wrangler@3.10.1/node_modules/wrangler/node_modules:/Users/nevendrean/code/elixir/up_img/ /node_modules/.pnpm/wrangler@3.10.1/node_modules:/Users/nevendrean/code/elixir/up_img/ /node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/wrangler.js" "$@"
else
  exec node  "$basedir/../../bin/wrangler.js" "$@"
fi
